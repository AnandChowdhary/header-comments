{
  "name": "header-comments",
  "version": "1.0.2",
  "description": "GitHub Action to add header comments to all your files",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "package": "ncc build --source-map",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koj-co/header-comments.git"
  },
  "keywords": [
    "github",
    "github actions",
    "nodejs",
    "typescript",
    "headers",
    "comments"
  ],
  "author": "Anand Chowdhary <mail@anandchowdhary.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/koj-co/header-comments/issues"
  },
  "homepage": "https://github.com/koj-co/header-comments#readme",
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^26.0.14",
    "@types/recursive-readdir": "^2.2.0",
    "@zeit/ncc": "^0.22.3",
    "jest": "^26.5.2",
    "semantic-release": "^17.1.2",
    "semantic-release-gitmoji": "^1.3.4",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "@koj/config": "^1.2.5",
    "recursive-readdir": "^2.2.2"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
